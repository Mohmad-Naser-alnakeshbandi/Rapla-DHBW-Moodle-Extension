{
    "manifest_version":3,
    "name":"DHBW-Extension",
    "description":"With the extension, the style DHBW-Karlshue-rapla is going to be changed ;)",
    "version":"1.0",
    "icons":{
        "16":"/images/icon-16.png",
        "48":"/images/icon-48.png",
        "128":"/images/icon-128.png"
    },
    "action":{
        "default_icon":{
            "16":"/images/icon-16.png",
            "48":"/images/icon-48.png",
            "128":"/images/icon-128.png"
        }
    },
    "content_scripts":[{
        "matches":["https://rapla.dhbw-karlsruhe.de/rapla?*"],
        "css":["main.css"],
        "js":["App.js"]
    }]
}